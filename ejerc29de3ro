		int intentos = 0;
		Scanner ingreso = new Scanner(System.in);
		do {
			System.out.println("ingrese su contra: ");
			contra = ingreso.nextInt();

			if (contra == 2008) {
				System.out.println("contrase単a correcta");
			}
			else if  (contra != 2008) {
				System.err.println("contrase単a incorrecta");
			}

		} while (contra != 2008);
	}

}
